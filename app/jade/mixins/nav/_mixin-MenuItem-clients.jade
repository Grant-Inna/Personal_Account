mixin main-clients ( parent, link1, link2 )
  li( class = "nav-item parent #{parent}" )
    a( href="#", role="button", aria-expanded="false").nav-link
      span.icon.s7-users
      span Мои клиенты
    ul.mai-nav-tabs-sub.mai-sub-nav.nav
      li.nav-item
        a( href="#", class = "nav-link #{link1}" )
          span.icon.icon-list
          span.name Списки клиентов
      li.nav-item
        a( href="#", class = "nav-link #{link2}" )
          span.icon.s7-download
          span.name Импорт